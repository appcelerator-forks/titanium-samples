".loadingMask": {
	backgroundColor: "#5000",
	visible: false
}

".loadingOuter": {
	width: Ti.UI.SIZE,
	height: Ti.UI.SIZE,

	borderRadius: 10,
	backgroundColor: "#C000"
}

